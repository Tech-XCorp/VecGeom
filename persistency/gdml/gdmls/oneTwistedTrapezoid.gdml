<?xml version="1.0"?>
<!-- \file oneTwistedTrapezoid.gdml -->
<!-- \brief geometry of one twisted trapezoid without material -->
<!-- \authors Author:  Dmitry Savin -->
<!-- \license Distributed under the Apache license 2.0 -->
<!-- unsupported by ROOT as of 6.13/03 version -->
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">

  <solids>
    <twistedtrd lunit="cm" aunit="rad" name="world" x1="90" x2="80" y1="60" y2="50" z="100" PhiTwist="1"/>
  </solids>

  <structure>
    <volume name="World">
      <solidref ref="world"/>
    </volume>
  </structure>

  <setup name="default" version="1.0" >
    <world ref="World" />
  </setup>

</gdml>
